<div class="main-content-area">
  <section class="page-title">
    <div class="container">
      <div class="row">
        <div class="col-md-6 col-sm-7 co-xs-12 text-left">
          <h1><%= t ".#{@type}.page_title" %></h1>
        </div>
        <div class="col-md-6 col-sm-5 co-xs-12 text-right">
          <div class="bread">
            <ol class="breadcrumb">
              <li><%= link_to t("page_title.home"), root_path %></a></li>
              <li class="active"><%= t ".#{@type}.breadcrumb" %></li>
            </ol>
          </div>
        </div>
      </div>
    </div>
  </section>
  <section class="white section-padding-80">
    <div class="container">
      <div class="row">
        <%= render partial: "users/user", collection: @users, as: :user %>
      </div>
      <div class="text-center clearfix">
        <%= paginate @users %>
      </div>
    </div>
  </section>
</div>
